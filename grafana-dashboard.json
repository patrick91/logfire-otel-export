{
  "dashboard": {
    "uid": "histogram-demo-logfire",
    "title": "Histogram Demo (Logfire + OTel)",
    "tags": ["logfire", "histogram", "opentelemetry"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Total Uploads (Counter)",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "logfire_file_uploads_total",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Upload Rate (files/sec)",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(logfire_uploaded_file_bytes_count[1m])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 2,
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Average File Size",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(logfire_uploaded_file_bytes_sum[5m]) / rate(logfire_uploaded_file_bytes_count[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "decimals": 0,
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "95th Percentile File Size",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(logfire_uploaded_file_bytes_bucket[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "decimals": 0,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 5000000,
                  "color": "yellow"
                },
                {
                  "value": 10000000,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "File Size Percentiles Over Time",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(logfire_uploaded_file_bytes_bucket[1m]))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(logfire_uploaded_file_bytes_bucket[1m]))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(logfire_uploaded_file_bytes_bucket[1m]))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Request Duration Percentiles by File Type",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(logfire_http_request_duration_seconds_bucket[1m])) by (file_type)",
            "refId": "A",
            "legendFormat": "{{file_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 7,
        "type": "bargauge",
        "title": "File Size Distribution (Cumulative Buckets)",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "logfire_uploaded_file_bytes_bucket",
            "refId": "A",
            "legendFormat": "{{le}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": []
        }
      },
      {
        "id": 8,
        "type": "piechart",
        "title": "Uploads by File Type",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (file_type) (rate(logfire_uploaded_file_bytes_count[5m]))",
            "refId": "A",
            "legendFormat": "{{file_type}}"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "pieType": "pie",
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 9,
        "type": "stat",
        "title": "Files by Type",
        "gridPos": {
          "x": 18,
          "y": 12,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "logfire_uploaded_file_bytes_count",
            "refId": "A",
            "legendFormat": "{{file_type}}"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "auto",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 10,
        "type": "heatmap",
        "title": "File Size Distribution Over Time (Heatmap)",
        "gridPos": {
          "x": 0,
          "y": 20,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(logfire_uploaded_file_bytes_bucket[1m])) by (le)",
            "refId": "A",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "options": {
          "calculate": false,
          "cellGap": 2,
          "color": {
            "scheme": "Spectral",
            "fill": "dark-orange",
            "mode": "scheme",
            "scale": "exponential"
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "bytes"
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Upload Counter by File Type",
        "gridPos": {
          "x": 0,
          "y": 28,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "logfire_file_uploads_total",
            "refId": "A",
            "legendFormat": "{{file_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      }
    ],
    "schemaVersion": 38,
    "version": 1
  },
  "overwrite": true
}
